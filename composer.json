{
    "name": "wprs/api",
    "description": "Provides an API for CIVL WPRS",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "wprs",
        "civl",
        "ranking"
    ],
    "authors": [
        {
            "name": "John Stevenson",
            "email": "john-stevenson@blueyonder.co.uk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "composer/ca-bundle": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.1",
        "johnstevenson/json-works": "2.1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Wprs\\Api\\": "src/"
        }

    },
    "autoload-dev": {
        "psr-4": {
            "Wprs\\Api\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "phpstan": "@php vendor/bin/phpstan analyse"
    }
}
